{
  "name": "goria-api",
  "version": "0.0.1",
  "private": true,
  "description": "Goria API Server",
  "repository": {
    "type": "git",
    "url": "https://github.com/as3ics/goria-api.git"
  },
  "license": "",
  "author": "zach@as3ics.com",
  "main": "index.js",
  "scripts": {
    "alphabatize": "npx sort-package-json",
    "build": "tsc",
    "build:dev": "tsc --watch --preserveWatchOutput true",
    "coverage": "cross-env NODE_ENV=test jest --silent -w 1 --coverage",
    "development": "pm2 start ecosystem.config.js --only goria-api-dev",
    "linecount": "npm run linecount:src && npm run linecount:test",
    "linecount:src": "cloc --by-file-by-lang src --include-lang=TypeScript ",
    "linecount:test": "cloc --by-file-by-lang tests --include-lang=TypeScript",
    "lint": "npm run lint:src",
    "lint:src": "tslint -c ./tslint.json 'src/**/*.ts'",
    "production": "pm2 start ecosystem.config.js --only goria-api",
    "serve": "nodemon src/index.ts",
    "start": "concurrently \"npm run build:dev\" \"nodemon --config nodemon-js.json dist/src/index.js\"",
    "stop": "pm2 delete all",
    "test": "cross-env NODE_ENV=test jest --silent -w 1",
    "test-all": "npm run typecheck && npm run lint && cross-env NODE_ENV=test jest --silent -w 4",
    "typecheck": "tsc -p . --noEmit true",
    "precommit": "npm run typecheck && npm run lint && npm run test"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "async": "^3.2.3",
    "async-retry": "^1.3.3",
    "aws-sdk": "^2.1112.0",
    "bcryptjs": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "*",
    "express-validator": "^6.1.4",
    "fs": "*",
    "git-rev": "^0.2.1",
    "hat": "^0.0.3",
    "jsbarcode": "^3.11.3",
    "jsonwebtoken": "^5.4.1",
    "mailgun-js": "^0.22.0",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.10",
    "mongoose-deep-populate": "^3.2.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nanoid": "^3.3.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "serve-handler": "^6.1.3",
    "square": "17.3.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.1",
    "@types/async": "^3.2.12",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-deep-populate": "^2.0.36",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "cloc": "^2.9.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "istanbul": "^0.4.5",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "sinon": "^13.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.6.3"
  },
  "plugins": [
    "square"
  ]
}
