    "fs": "*",
    "git-rev": "^0.2.1",
    "hat": "^0.0.3",
    "jsbarcode": "^3.11.3",
    "jsonwebtoken": "^5.4.1",
    "mailgun-js": "^0.22.0",
    "mongodb": "^4.4.1",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nanoid": "^3.3.2",
